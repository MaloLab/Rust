fn main() {
    let point = (0, 0); // Change values to test

    match point {
        (0, 0) => println!("origin"),
        (0, _) => println!("y_axis"),
        (_, 0) => println!("x_axis"),

        (x, y) if x > 0 && y > 0 => println!("Q1"),
        (x, y) if x < 0 && y > 0 => println!("Q2"),
        (x, y) if x < 0 && y < 0 => println!("Q3"),
        (x, y) if x > 0 && y < 0 => println!("Q4"),

        (_,_) => println!("error"),
    }
}
