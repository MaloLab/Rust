fn main() {
    let numbers = vec![10, 23, 30, 45, 50];
    let mut even_numbers: Vec<i32> = Vec::new();

    for n in &numbers {
        if n % 2 == 0 {
            even_numbers.push(*n);
        }
    }

    println!("numbers = {:?}", numbers);
    println!("even_numbers = {:?}", even_numbers);
}
