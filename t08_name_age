use std::collections::HashMap;

fn main(){

    let mut people: HashMap<String, u32> = HashMap::new();

    people.insert("Malo".to_string(), 21);
    people.insert("Rayan".to_string(), 22);
    people.insert("Fethi".to_string(), 21);
    people.insert("Gab".to_string(), 20);
    people.insert("Malek".to_string(), 21);
    people.insert("Noyan".to_string(), 21);

    for (key, value) in &people {
        println!(" {}: {}", key, value);
    }

}
